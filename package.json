{
  "author": "Allan Avelar <contact@allanavelar.com>",
  "name": "mercadolivre-frontend-test",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint .",
    "postinstall": "npm run build",
    "start-prod": "set NODE_ENV=production && node dist/server/index.js",
    "start-dev": "set NODE_ENV=development && nodemon --watch src/server src/server/index.js --exec babel-node",
    "build": "set NODE_ENV=production && babel src --out-dir dist && babel-node ./node_modules/.bin/webpack"
  },
  "dependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-node-env-inline": "^0.0.2",
    "babel-preset-env": "^1.1.11",
    "babel-preset-react": "^6.23.0",
    "bootstrap": "4.0.0-alpha.6",
    "css-loader": "^0.26.2",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "lodash": "^4.17.4",
    "node-fetch": "^1.6.3",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.3",
    "prop-types": "^15.6.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-router": "^3.0.2",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-hot-middleware": "^2.17.1"
  },
  "devDependencies": {
    "eslint": "^3.16.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-sort-class-members": "^1.1.1",
    "nodemon": "^1.11.0",
    "webpack-dev-middleware": "^1.10.1"
  }
}
